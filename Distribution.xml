    <line choice="default">
      <line choice="org.openkms.mac.ccid"/>
    </line>
  </choices-outline>
  <choice id="default"/>
  <choice id="org.openkms.mac.ccid" visible="false">
    <pkg-ref id="org.openkms.mac.ccid"/>
  </choice>
  <pkg-ref id="org.openkms.mac.ccid" onConclusion="RequireRestart">ifd-ccid-openkms.pkg</pkg-ref>
</installer-gui-script>
