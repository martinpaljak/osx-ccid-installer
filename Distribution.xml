  </allowed-os-versions>
  <license file="license.rtf" mime-type="text/rtf"/>
  <welcome file="welcome.rtf" mime-type="text/rtf"/>
  <title>CCID free software driver</title>
  <pkg-ref id="org.openkms.mac.ccid"/>
  <options customize="never" require-scripts="false"/>
